{
  "name": "next-full-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build && npm gql:generate",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start",
    "format": "prettier --write .",
    "gql:generate": "graphql-codegen --require dotenv/config --config codegen.ts",
    "gql:generate:dev": "graphql-codegen --require dotenv/config --config codegen.ts --watch",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --passWithNoTests --watch",
    "cypress:open": "cypress open",
    "e2e": "start-server-and-test dev http://localhost:3000 \"cypress open --e2e\"",
    "e2e:headless": "start-server-and-test dev http://localhost:3000 \"cypress run --e2e\""
  },
  "dependencies": {
    "@apollo/client": "^3.11.4",
    "@apollo/experimental-nextjs-app-support": "^0.11.2",
    "@datadog/browser-rum": "^5.23.3",
    "@reduxjs/toolkit": "^2.2.7",
    "@t3-oss/env-nextjs": "^0.11.0",
    "geist": "^1.3.1",
    "graphql": "^16.9.0",
    "next": "^14.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "server-only": "^0.0.1",
    "superjson": "^2.2.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@0no-co/graphqlsp": "^1.12.13",
    "@biomejs/biome": "1.8.3",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/schema-ast": "^4.1.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
    "@jest/globals": "^29.7.0",
    "@parcel/watcher": "^2.4.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@types/eslint": "^8.56.11",
    "@types/node": "^20.16.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.1.0",
    "cypress": "^13.13.3",
    "daisyui": "^4.12.10",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "start-server-and-test": "^2.0.5",
    "tailwindcss": "^3.4.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "packageManager": "pnpm@8.15.8"
}
